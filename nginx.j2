user www-data;
worker_processes auto;
pid /run/nginx.pid;
include /etc/nginx/modules-enabled/*.conf;

events {
        worker_connections 768;
        # multi_accept on;
}
stream {
  upstream syslog_udp {
    {% for servers in groups["webservers"] %}
      server {{ hostvars[servers]['ansible_default_ipv4']['address'] }}:161;
    {% endfor %}
  }
  server {
    listen 1611 udp;
    proxy_pass syslog_udp;
  }
}
