Host bastion
        User ubuntu
        HostName 103.57.74.244
        IdentityFile /home/varunduvva/.ssh/id_rsa
        ForwardAgent yes
        UserKnownHostsFile ~/dev/null
        StrictHostKeyChecking no
        PasswordAuthentication no
        #ControlMaster auto
        #ControlPersist 5m


Host HAproxy
        User ubuntu
        HostName 188.95.226.151
        IdentityFile /home/varunduvva/.ssh/id_rsa
        UserKnownHostsFile=~/dev/null
        StrictHostKeyChecking no
        PasswordAuthentication no
        ProxyCommand ssh -W %h:%p bastion

